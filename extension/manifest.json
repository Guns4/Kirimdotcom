{
    "manifest_version": 3,
    "name": "CekKirim Resi Highlighter",
    "version": "1.0",
    "description": "Automatically detects and links tracking numbers (JNE, J&T, SiCepat) to CekKirim.com",
    "icons": {
        "48": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "permissions": []
}